{
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "amd": true,
    "commonjs": true,
    "mocha": true
  },
  "globals": {
    "react": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "for-direction": 1,
    "no-await-in-loop": 0,
    "no-compare-neg-zero": 1,
    "no-cond-assign": 1,
    "no-console": 0,
    "no-constant-condition": 1,
    "no-control-regex": 0,
    "no-debugger": 0,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty": 1,
    "no-empty-character-class": 0,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 0,
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-obj-calls": 0,
    "no-prototype-builtins": 0,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 0,
    "no-template-curly-in-string": 1,
    "no-unexpected-multiline": 1,
    "no-unreachable": 1,
    "no-unsafe-finally": 0,
    "no-unsafe-negation": 1,
    "use-isnan": 1,
    "valid-jsdoc": 1,
    "valid-typeof": 1,
    
    "strict": 0,
    
    "no-undef": 0,
    "no-undef-init": 2,
    "no-undefined": 1,
    "no-unused-vars": 0,
    "no-use-before-define": 2,
    
    "global-require": 0,
    "handle-callback-err": 1,
    "no-buffer-constructor": 1,
    
    "array-bracket-spacing": 0,
    "brace-style": 1,
    "max-depth": 1,
    "max-nested-callbacks": 1,
    "linebreak-style": 0,
    "no-lonely-if": 1,
    
    "constructor-super": 1,
    "generator-star-spacing": 0,
    "no-this-before-super": 1,
    "no-var": 1,
    "object-shorthand": 0,
    "prefer-const": 0
  }
}